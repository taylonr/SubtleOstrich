@model IEnumerable<SubtleOstrich.Logic.Occurrence>

@{
    ViewBag.Title = "Activity";
}
<script src="@Url.Content("~/Scripts/subtleOstrich/activity.js")"></script>
<div class="row">
    <div class="well" ng-controller="ActivityControl">
        <ul>
            <li ng-repeat="act in activity">
                <span>{{act.name}} - {{act.note}}</span>
            </li>
        </ul>
        
    </div>
</div>


